<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="StakingAmountView">
        <attr name="android:enabled" />
    </declare-styleable>
    <declare-styleable name="EstimateEarningView" />
    <declare-styleable name="StakingGainView">
        <attr name="titleText" format="string" />
    </declare-styleable>

    <declare-styleable name="PayoutTargetView">
        <attr name="android:checked" />
        <attr name="targetName" format="string" />
        <attr name="targetDescription" format="string" />
        <attr name="targetAmountDescription" format="string" />
    </declare-styleable>
    <declare-styleable name="ValidatorInfoBlockView">
        <attr name="validatorInfoTitle" format="string" />
        <attr name="showExtra" format="boolean" />
        <attr name="showInfoIcon" format="boolean" />
    </declare-styleable>
    <declare-styleable name="ValidatorIdentityItemView">
        <attr name="validatorIdentityTitle" format="string" />
        <attr name="showArrowIcon" format="boolean" />
    </declare-styleable>
</resources>